{
    "version": 2,
    "functions": {
      "./index.js": {
        "runtime": "@now/node",
        "memory": 512,
        "maxDuration": 30
      }
    },
    "builds": [
      {
        "src": "./index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "./index.js"
      }
    ]
  }